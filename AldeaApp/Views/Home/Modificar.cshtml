<div class="centrado2">
    <h2>Modificar usuario</h2>
    <select id="Tipoid" class="form-control" placeholder="Tipo de documento">
        <option value="" disabled selected hidden>Elegir tipo de documento</option>
        <option value="CC">Cédula de Ciudadanía</option>
        <option value="CE">Cédula de Extranjería</option>
        <option value="NIT">Número de Identificación Tributaria</option>
    </select>
    <input type="text" id="IdBusqueda" class="form-control" autocomplete="off" maxlength="50" placeholder="Documento" required />
    <button class="ui-button ui-widget ui-corner-all" id="btnBuscar" value="Buscar registro" type="submit">Buscar registro</button>
    </div>

<div class="container-fluid" id="contenedor">
    <div id="DivRegistro">
        <h3>Modificación de datos de usuario.</h3>
        <h4>Por favor diligencie los campos a continuación.</h4>
    </div>
    <form action="" method="POST">
        <div class="row">

            <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                <div class="form-group">
                    <label class="labelLeyenda">Tipo de documento</label>
                    <select id="Tipoid" class="form-control">
                        <option value="CC">Cédula de Ciudadanía</option>
                        <option value="CE">Cédula de Extranjería</option>
                        <option value="NIT">Número de Identificación Tributaria</option>
                    </select>

                </div>
            </div>

            <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                <div class="form-group">
                    <label class="labelLeyenda">Documento</label>
                    <input type="text" id="NumId" value="" class="form-control" autocomplete="off" maxlength="50" required />
                </div>
            </div>
            <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                <div class="form-group">
                    <label class="labelLeyenda">Nombres</label>
                    <input type="text" id="NomUsuario" value="" class="form-control" autocomplete="off" maxlength="100" required />
                </div>
            </div>

            <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                <div class="form-group">
                    <label class="labelLeyenda">Apellidos</label>
                    <input type="text" id="ApellidosUsuario" value="" class="form-control" autocomplete="off" maxlength="100" required />
                </div>
            </div>

            <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                <div class="form-group">
                    <label class="labelLeyenda">Fecha de nacimiento</label>
                    <input type="text" id="FechaNacimiento" class="form-control" autocomplete="off" maxlength="50">
                </div>
            </div>

            <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                <div class="form-group">
                    <label class="labelLeyenda">Ciudad de nacimiento</label>
                    <input type="text" id="CiudadNacimiento" value="" class="form-control" autocomplete="off" maxlength="50" required />
                </div>
            </div>
            <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
